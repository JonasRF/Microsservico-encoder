DB_TYPE="postgres"
DSN="dbname=encoder sslmode=disable user=postgres password=root host=localhost"

DB_TYPE_TEST="sqlite3"
DSN_TEST=":memory:"

ENV="dev"
DEBUG=true
AUTO_MIGRATE_DB=true

localStoragePath="/tmp"
inputBucketName="projetotccencoder"
outputBucketName="projetotccencoder"
CONCURRENCY_UPLOAD=50

RABBITMQ_DEFAULT_USER=adm_videos
RABBITMQ_DEFAULT_PASS=123456
RABBITMQ_DEFAULT_HOST=rabbitmq
RABBITMQ_DEFAULT_PORT=5672
RABBITMQ_DEFAULT_VHOST=/
RABBITMQ_CONSUMER_QUEUE_NAME=video.encoded.queue
RABBITMQ_CONSUMER_NAME=app-name
RABBITMQ_NOTIFICATION_EX=amqp.direct
RABBITMQ_NOTIFICATION_ROUTING_KEY=video.encoded
RABBITMQ_DLX=video.events

GOOGLE_APPLICATION_CREDENTIALS="bucket-credential.json"